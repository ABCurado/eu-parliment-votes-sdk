{
  "name": "eu-parliment-sdk",
  "version": "1.4.0",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "clean": "node tools/cleanup",
    "package": "npm run build && npm pack",
    "test": "jest ",
    "test:full": "jest --no-cache --runInBand --detectOpenHandles --detectLeaks --forceExit",
    "test:cov": "jest --coverage --no-cache --runInBand",
    "addscope": "node tools/packagejson name @abcurado/eu-parliment-sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "data",
    "typescript",
    "npm",
    "package",
    "open-data"
  ],
  "author": "ABCurado <antonio.b.curado@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ABCurado/eu-parliment-sdk",
  "repository": {
    "type": "git",
    "url": "git@github.com:ABCurado/eu-parliment-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ABCurado/eu-parliment-sdk/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^29.6.4",
    "pinst": "^2.1.6",
    "prettier": "^2.4.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.2.5",
    "typescript": "^5.2.2",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@types/node": "^20.5.9",
    "node-html-parser": "^6.1.8",
    "openai": "^4.17.4"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  }
}
